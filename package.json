{
  "name": "@slimio/tarball",
  "version": "0.1.0",
  "description": "SlimIO archive tarball packer/extractor",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "prepublishOnly": "pkg-ok",
    "test": "cross-env psp && node test/test.js",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=html",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/Tarball.git"
  },
  "keywords": [
    "slimio",
    "archive",
    "bundler",
    "pack",
    "packer",
    "tarball",
    "extractor",
    "extract"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/Tarball/issues"
  },
  "homepage": "https://github.com/SlimIO/Tarball#readme",
  "dependencies": {
    "tar-fs": "^2.0.0",
    "uuid": "^3.3.3",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^3.0.3",
    "@slimio/psp": "^0.8.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "husky": "^3.0.9",
    "japa": "^3.0.1",
    "jsdoc": "^3.6.3",
    "nyc": "^14.1.1",
    "pkg-ok": "^2.3.1"
  },
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env eslint index.js && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 90
  },
  "snyk": true
}
